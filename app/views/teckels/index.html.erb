
<% @teckels.each do |teckel| %>
  <%= link_to teckel.name, teckel_path(teckel) %>
  <%= cl_image_tag teckel.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>

<%= link_to "create", new_teckel_path %>

<% if user_signed_in? %>
  <%= link_to "My profile", profile_path %>
  <%= link_to "Logout",  destroy_user_session_path, method: :delete %>
  <% else %>
  <%= link_to "Login", user_session_path %>
<% end %>
